{
  "include": [
    "backend"
  ],
  "exclude": [
    "**/__pycache__",
    ".venv",
    "venv",
    "build",
    "dist",
    "node_modules"
  ],
  "extraPaths": ["backend"],
  "pythonVersion": "3.10",
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportUnusedImport": true,
  "reportUnusedVariable": true
}

